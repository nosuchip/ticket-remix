{
  "name": "ticket-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "db:migrate": "drizzle-kit generate:pg",
    "db:apply": "tsx ./app/db/migrate.server",
    "db:seed": "tsx ./app/db/seed.server",
    "format": "prettier --write .",
    "format:repo": "yarn format && yarn lint -- --fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@yudiel/react-qr-scanner": "^1.2.6",
    "auth0": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "cloudinary": "^1.41.3",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "ethers": "^6.10.0",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "isbot": "^4.1.0",
    "pg-connection-string": "^2.6.2",
    "postcss": "^8.4.33",
    "postgres": "^3.4.3",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.1",
    "remix-auth": "^3.6.0",
    "remix-auth-auth0": "^1.9.0",
    "remix-utils": "^7.5.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.20.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ]
}
